{
  "name": "Mobilly",
  "scripts": {
    "test": "jest --no-cache --coverage",
    "clean": "trash frontend/dist .awcache coverage",
    "dev": "yarn clean && yarn dll && concurrently \"yarn dev-backend\" \"yarn dev-frontend\"",
    "dev-backend": "cross-env NODE_ENV=development rm -rf ./backend/dist && webpack --w --config webpack.config.backend.babel.js",
    "dev-frontend": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.frontend.babel.js",
    "dll": "cross-env NODE_ENV=production webpack --config webpack.config.frontend.dll.babel.js"
  },
